<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <stroke
                android:width="2px"
                android:color="#f0f0f0" />
            <!-- 边框颜色 -->
            <solid android:color="#ffffffff" />
            <!-- 填充色 -->
            <corners android:radius="10dp" />

            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>

</layer-list>